#include <bits/stdc++.h>
using namespace std;

int main() 
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n,k;
    cin>>n>>k;
    vector<int>v(n);vector<int>temp;
    for(int i=0;i<n;i++)cin>>v[i];
    int l=0;int r=0;map<int,int>mp;
    for(int i=0;i<k;i++)
    {
        mp[v[r]]++;
        r++;
    }
    temp.push_back(mp.size());
    while(r<n){
          mp[v[r]]++;
          mp[v[l]]--;
          if(mp[v[l]]==0)mp.erase(v[l]);
          r++;
          temp.push_back(mp.size());
    }
    for(auto it:temp)cout<<it<<" ";
    
}
